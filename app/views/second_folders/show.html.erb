<h3>詳細</h3>
<div class="second_folder">
  <p><%= @second_folder.folder %></p>
  <p><%= @second_folder.created_at %></p>
</div>

<div class="second_folders-container">
  <% @second_folders.each do |t| %>
    <div class="second_folder">
      <%= t.two_title %>
      <%= t.two_body %>
    </div>
  <% end %>
</div>

<%= link_to "一覧に戻る", folders_path %>

